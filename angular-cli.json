{
  "project": {
    "version": "1.0.0-beta.14",
    "name": "infographics-server"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": "assets",
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "styles.scss",
        "../node_modules/bourbon/app/assets/stylesheets/_bourbon.scss",
        "../node_modules/bourbon-neat/app/assets/stylesheets/_neat.scss",
        "../node_modules/bourbon-neat/app/assets/stylesheets/_neat-helpers.scss",
        "../node_modules/font-awesome/scss/font-awesome.scss"

      ],
      "scripts": [],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false
  }
}
